{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "start": "node dist/index.bundle.js",
    "dev": "webpack --mode development --watch",
    "types": "ts-node scripts/generate-api-types/generateTypes.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier src --check",
    "format:fix": "prettier src --write",
    "docs": "npm run docs:lint && npm run docs:bundle && npm run docs:build",
    "docs:lint": "redocly lint ./src/api/docs/index.yaml",
    "docs:bundle": "redocly bundle src/api/docs/index.yaml -o src/api/swagger.yaml",
    "docs:build": "redocly build-docs src/api/docs/index.yaml -o src/api/routes/docs/index.html",
    "test": "npm run spec:unit && npm run spec:int",
    "spec:unit": "jest --config ./jest.unit.config.ts",
    "spec:int": "jest --config ./jest.int.config.ts --detectOpenHandles",
    "db:format": "prisma format",
    "db:migrate": "prisma migrate dev --name init"
  },
  "private": true,
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.2.0",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/yamljs": "^0.2.34",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.6.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-transform-yaml": "^1.1.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "nodemon": "^3.1.3",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "3.3.2",
    "prisma": "^5.16.1",
    "prisma-types-export": "^1.0.0",
    "supertest": "^7.0.0",
    "swagger-typescript-api": "^13.0.11",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.2.1",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "yamljs": "^0.3.0"
  }
}
