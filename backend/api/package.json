{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "types": "ts-node scripts/generate-api-types/generateTypes.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier src --check",
    "format:fix": "prettier src --write",
    "docs": "npm run docs:lint & npm run docs:bundle & npm run docs:build",
    "docs:lint": "redocly lint ./src/docs/index.yaml",
    "docs:bundle": "redocly bundle src/docs/index.yaml -o src/swagger.yaml",
    "docs:build": "redocly build-docs src/docs/index.yaml -o src/routes/docs/index.html",
    "spec:unit": "jest --config ./jest.unit.config.ts",
    "spec:int": "jest --config ./jest.int.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.2.0",
    "http-status-codes": "^2.3.0",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "3.3.2",
    "supertest": "^7.0.0",
    "swagger-typescript-api": "^13.0.11",
    "test": "file:lib/test",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "_moduleAliases": {
    "@services": "dist/src/services"
  }
}
